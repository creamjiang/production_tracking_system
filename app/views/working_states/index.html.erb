
<h1>Listing working_states</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Bin</th>
    <th>Routing procedure</th>
    <th>Employee</th>
    <th>Good unit</th>
    <th>Reject unit</th>
  </tr>

<% @working_states.each do |working_state| %>
  <tr class="<%= cycle("list0", "list1")%>">
    <td><%=h working_state.product_id %></td>
    <td><%=h working_state.bin_id %></td>
    <td><%=h working_state.routing_procedure_id %></td>
    <td><%=h working_state.employee_id %></td>
    <td><%=h working_state.good_unit %></td>
    <td><%=h working_state.reject_unit %></td>
    <td class="action"><%= link_to 'Show', working_state %></td>
  </tr>
<% end %>
</table>

<br />


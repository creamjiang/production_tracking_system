<h1>Editing operator</h1>

<% form_for(@operator) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :machine_id %><br />
    <%= f.text_field :machine_id %>
  </p>
  <p>
    <%= f.label :employee_id %><br />
    <%= f.text_field :employee_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @operator %> |
<%= link_to 'Back', operators_path %>
<% form_for @procedure_product do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :generic_name %><br />
    <%= f.text_field :generic_name %>
  </p>
  
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :size => "60x4" %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>

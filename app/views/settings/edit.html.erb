<h1>Editing setting</h1>

<% form_for(@setting) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :clear_bin_with_scanner %><br />
    <%= f.check_box :clear_bin_with_scanner %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @setting %> |
<%= link_to 'Back', settings_path %>